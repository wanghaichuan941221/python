<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="local_joint_control_type" default=""/>
  <arg name="ns" default=""/>

  <!-- Configure local joint control -->
  
  <!-- Left leg -->
  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_1_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_2_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_3_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_4_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_5_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_6_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_arm_7_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_head_1_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_head_2_joint.yaml" />

  <rosparam command="load"
    ns="$(arg ns)/joints"
    file="$(find tiago_controller_configuration)/config/local_joint_control/$(arg local_joint_control_type)/$(arg local_joint_control_type)_torso_lift_joint.yaml" />


</launch>
